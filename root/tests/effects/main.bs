use lang:bs:effects;
use lang:bs:macro;
use test;

effect: Int myEffect(Int x);

handler: Int MyHandler(Int) {
	myEffect(Int x), k {
		x;
	}
	return x {
		x + 1;
	}
}

suite Basic {
	Int w = handle {
		Int x = 0; //myEffect(10);
		x + 10;
	} with MyHandler;

	test w == 11;
	print("Result: ${w}");
}
