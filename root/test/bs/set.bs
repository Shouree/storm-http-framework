use core:debug;

Int iterateSet(Set<Int> data) {
	Int r = 0;
	for (k in data) {
		r += k;
	}
	r;
}

Int iterateWeakSetPlain(WeakSet<DbgActor> data) {
	Int r = 0;
	var iter = data.iter;
	while (k = iter.next) {
		r += k.get();
	}

	r;
}

