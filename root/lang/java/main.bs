use lang;
use core:io;
use core:lang;
use lang:bs:macro;
use lang:bnf;

PkgReader reader(Url[] files, Package pkg) {
	FilePkgReader(files, pkg, &createFile(FileInfo));
}

FileReader createFile(FileInfo info) {
	JavaReader(info);
}

class JavaReader extends FileReader {
	ctor(FileInfo info) {
		init(info);
	}

	Rule rootRule() {
		named{SRoot};
	}
}
